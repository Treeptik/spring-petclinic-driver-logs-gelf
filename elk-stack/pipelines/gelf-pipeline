{
  "description" : "Ingest pipeline for Gelf format messag",
  "processors": [
        {
          "grok": {
            "field": "message",
            "patterns": [
              "%{GREEDYDATA:message}"
            ]
          }
        },
        {
          "json": {
            "field": "message",
            "add_to_root": true
          }
        }
      ]
    }
}'




